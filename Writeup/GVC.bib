@article{Kummritz20161,
abstract = {Global Value Chains (GVCs) have become a central topic in trade and develop-ment policy but little is known about their actual impact on economic performance. Using a new unique set of Inter-Country Input-Output tables with extensive coun-try coverage, I show that an increase in GVC participation leads to higher domestic value added and productivity for all countries independent of their income levels. Causality is established by building a novel value added trade resistance index that combines third country trade costs with industry-specific technological variables leading to an exogenous variation in GVC participation. Based on the preferred IV specification, I find that a 1 percent increase in backward GVC participation leads to 0.11{\%} higher domestic value added in the average industry. Equally, I find for forward linkages that a 1 percent increase in GVC participation leads to 0.60{\%} higher domestic value added and to 0.33{\%} higher labour productivity.},
author = {Kummritz, Victor},
doi = {10.1002/pd},
file = {:C$\backslash$:/Users/Sebastian Krantz/Documents/Geneva Graduate Institute/Dissertation/Input-Output Analysis/CTEI-2016-01.pdf:pdf},
journal = {CTEI Working Paper No 2016-01},
mendeley-groups = {Dissertation/Input-Output Analysis},
title = {{Do Global Value Chains Cause Industrial Development?}},
url = {http://repec.graduateinstitute.ch/pdfs/cteiwp/CTEI-2016-01.pdf},
year = {2016}
}

